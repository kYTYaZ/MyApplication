<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        <com.andy.myapplication.Title.TitleLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
        />

        <Button
                android:text="GridRecyclerView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/GridRecyclerView"
                android:textAllCaps="false"
                android:textSize="20sp"
                android:textColor="@color/区号代码字体"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"/>
        <Button
                android:text="PuRecyclerView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/PuRecyclerView"
                android:textAllCaps="false"
                android:textSize="20sp"
                android:textColor="@color/区号代码字体"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"/>
        <Button
                android:text="WebView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/WebView"
                android:textAllCaps="false"
                android:textSize="20sp"
                android:textColor="@color/区号代码字体"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
        />
        <Button
                android:text="Toast"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/Toast"
                android:textAllCaps="false"
                android:textSize="20sp"
                android:textColor="@color/区号代码字体"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
        />
        <Button
                android:text="Dialog"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/Dialog"
                android:textAllCaps="false"
                android:textSize="20sp"
                android:textColor="@color/区号代码字体"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
        />
        <Button
                android:text="Esc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/Esc"
                android:textAllCaps="false"
                android:textSize="20sp"
                android:textColor="@color/区号代码字体"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
        />
        <Button
                android:text="HorRecyclerView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/HorRecyclerView"
                android:textAllCaps="false"
                android:textSize="20sp"
                android:textColor="@color/区号代码字体"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
        />
        <Button
                android:text="ListView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ListView"
                android:textSize="20sp"
                android:textColor="@color/区号代码字体"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
                android:textAllCaps="false"
        />

        <Button
                android:text="ProgressDialog"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ProgressDialog"
                android:textSize="20sp"
                android:textColor="@color/区号代码字体"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
                android:textAllCaps="false"
        />

        <Button
            android:text="Chat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Chat"
            android:textSize="20sp"
            android:textColor="@color/区号代码字体"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginStart="10dp"
            android:textAllCaps="false"
            />

        <Button
            android:text="Main"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Main"
            android:textSize="20sp"
            android:textColor="@color/区号代码字体"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginStart="10dp"
            android:textAllCaps="false"
            />

        <Button
            android:text="NewsFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/NewsFragment"
            android:textSize="20sp"
            android:textColor="@color/区号代码字体"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginStart="10dp"
            android:textAllCaps="false"
            />
        <Button
            android:text="Test_Broadcast"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Test_Broadcast"
            android:textSize="20sp"
            android:textColor="@color/区号代码字体"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginStart="10dp"
            android:textAllCaps="false"
            />

        <Button
            android:text="Test_Broadcast2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Test_Broadcast2"
            android:textSize="20sp"
            android:textColor="@color/区号代码字体"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginStart="10dp"
            android:textAllCaps="false"
            />
        <Button
                android:text="Test_Broadcast3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/Test_Broadcast3"
                android:textSize="20sp"
                android:textColor="@color/区号代码字体"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
                android:textAllCaps="false"
        />

        <Button
                android:text="Test_Broadcast4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/Test_Broadcast4"
                android:textSize="20sp"
                android:textColor="@color/区号代码字体"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
                android:textAllCaps="false"
        />
    </LinearLayout>
</ScrollView>